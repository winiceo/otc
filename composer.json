{
  "name": "genvotc",
  "description": "",
  "keywords": [
    "web",
    "php",
    "laravel",
    "app"
  ],
  "license": "Apache-2.0",
  "type": "project",
  "repositories": [
    {
      "type": "artifact",
      "url": "resources/repositorie/zips/"
    },
    {
      "type": "path",
      "url": "packages/genv-plus-id",
      "options": {
        "symlink": true,
        "plus-soft": true
      }
    },
    {
      "type": "path",
      "url": "packages/genv-web",
      "options": {
        "symlink": true,
        "plus-soft": true
      }
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "doctrine/dbal": "^2.5",
    "fideloper/proxy": "~3.3",
    "genv/web": "1.1.0",
    "guzzlehttp/guzzle": "^6.3",
    "intervention/image": "^2.3",
    "jpush/jpush": "^3.5",
    "laravel/framework": "5.5.*",
    "overtrue/easy-sms": "^0.0.10",
    "pingplusplus/pingpp-php": "^2.2",
    "predis/predis": "^1.1",
    "genv/plus-id": "1.1.0",
    "symfony/finder": "~3.3",
    "symfony/polyfill-mbstring": "^1.4",
    "symfony/yaml": "^3.3",
    "tymon/jwt-auth": "1.0.0-rc.1"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "codacy/coverage": "^1.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "Genv\\Otc\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Genv\\Otc\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    },
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
